import java.applet.*;
import java.awt.*;

/*<Applet code="LineRectCircle.class" height=500 width=500></Applet>*/
public class LineRectCircle extends Applet
{
	public void paint(Graphics g)
	{
		g.drawLine(50,50,150,50);

		g.drawRect(50,80,150,150);

		g.setColor(Color.green);
		g.fillOval(50,250,120,120);
	}
}
