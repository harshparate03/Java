import java.applet.*;
import java.awt.*;

/*<Applet code="Radius.class" height=300 width=300>
<param NAME=radi value=100></param>
</Applet>*/

public class Radius extends Applet
{	
	public void paint(Graphics g)
	{
		String s;
		int r;
		s=getParameter("radi");
		r=Integer.parseInt(s);

		g.drawOval(100,80,r,r);
	}
}
