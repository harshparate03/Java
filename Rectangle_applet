import java.applet.*;
import java.awt.*;

/*<Applet code="Rectangle.class" height=500 width=500 ></Applet>*/

public class Rectangle extends Applet
{
	public void paint(Graphics g)
	{
		g.drawRect(140,160,220,130); 

		g.drawLine(184,160,184,290);
		g.drawLine(228,160,228,290);
		g.drawLine(272,160,272,290);
		g.drawLine(318,160,318,290);
	}
}
